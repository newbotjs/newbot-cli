{
  "name": "newbot-cli",
  "version": "2.3.3",
  "description": "CLI for NewBot Framework. Quickly create, generate, emulate and test your chatbot",
  "main": "./dist/bin/main.js",
  "bin": {
    "newbot": "./dist/bin/main.js"
  },
  "ava": {
    "require": [
      "@babel/register"
    ]
  },
  "scripts": {
    "prepublish": "npm run build",
    "install": "node dist/install/gactions",
    "start": "newbot serve",
    "serve": "babel -w src/ -d dist/",
    "build": "babel src/ -d dist/",
    "test": "npx ava tests/*.spec.js"
  },
  "keywords": [
    "newbot",
    "chatbot",
    "messenger",
    "assistant",
    "viber"
  ],
  "author": "Samuel Ronce",
  "license": "MIT",
